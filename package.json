{
    "name": "scibowl-org",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "description": "Science Bowl Tournament Management Platform",
    "workspaces": [
        "shared",
        "apps/website/frontend",
        "apps/moss/frontend"
    ],
    "scripts": {
        "install-all": "npm install",
        "dev:website": "cd apps/website/frontend && npm run dev",
        "dev:moss": "cd apps/moss/frontend && npm run dev",
        "build:website": "cd apps/website/frontend && npm run build",
        "build:moss": "cd apps/moss/frontend && npm run build",
        "build": "npm run build:website && npm run build:moss",
        "lint": "npm run -w shared lint && npm run -w apps/website/frontend lint && npm run -w apps/moss/frontend lint",
        "types-check": "tsc --noEmit"
    },
    "devDependencies": {
        "fdir": "^6.5.0",
        "typescript": "~5.9.3"
    }
}
